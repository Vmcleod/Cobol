     05 WS-DCHR74           PIC X.                             
     05 WS-DCHR75           PIC X.                             
     05 WS-DCHR76           PIC X.                             
     05 WS-DCHR77           PIC X.                             
     05 WS-DCHR78           PIC X.                             
     05 WS-DCHR79           PIC X.                             
     05 WS-DCHR80           PIC X.                             
                                                                 
 PROCEDURE DIVISION.                                           
                                                                 
     PERFORM 0100-INITIALIZE.                                  
     PERFORM 0200-LOAD-ALPHA-TABLE UNTIL NO-MORE-LETTERS.      
     PERFORM 0300-DECODE-MSG.                                  
     PERFORM 0400-END-ROUTINE.                                 
                                                                 
    0100-INITIALIZE.                                           
     OPEN INPUT LETTERFILE ENCODEFILE                          
          OUTPUT DECODEFILE.                                   
                                                                 
    0200-LOAD-ALPHA-TABLE.                                     
          PERFORM 0225-ALPHA-CODES.                            
           IF MORE-LETTERS                                     
             PERFORM 0250-PROCESS-LETTERS.                     
                                                                 
    0225-ALPHA-CODES.                                          
          READ LETTERFILE                                      
           AT END MOVE 'N'     TO LETTER-SWITCH                
                  MOVE 1       TO A-SUB.                       
                                                                 
    0250-PROCESS-LETTERS.                                      
          ADD +1               TO A-SUB                        
          MOVE LETTER-NAME     TO ABC-CHAR(A-SUB).             
          MOVE CODES           TO NUM-CODE(A-SUB).             
          DISPLAY                                              
         'ALPHA ' ABC-CHAR(A-SUB) ' CODE ' NUM-CODE(A-SUB).    
         
    0300-DECODE-MSG.                                            
          READ ENCODEFILE.                                       
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR01.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR01.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR02.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR02.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR03.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR03.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR04.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR04.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR05.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR05.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR06.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR06.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR07.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR07.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR08.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR08.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR09.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR09.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR10.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR10.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR11.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR11.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR12.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR12.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR13.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR13.                     
          INITIALIZE SUBSCRIPTS.  
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR14.
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR14.                   
          INITIALIZE SUBSCRIPTS.                               
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR15.
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR15.                   
          INITIALIZE SUBSCRIPTS.                               
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR16.
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR16.                   
          INITIALIZE SUBSCRIPTS.                               
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR17.
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR17.                   
          INITIALIZE SUBSCRIPTS.                               
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR18.
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR18.                   
          INITIALIZE SUBSCRIPTS.                               
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR19.
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR19.                   
          INITIALIZE SUBSCRIPTS.                               
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR20.
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR20.                   
          INITIALIZE SUBSCRIPTS.                               
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR21.
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR21.                   
          INITIALIZE SUBSCRIPTS.                               
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR22.
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR22.                   
          INITIALIZE SUBSCRIPTS.                               
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR23.
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR23.                   
          INITIALIZE SUBSCRIPTS.                               
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR24.
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR24.                   
          INITIALIZE SUBSCRIPTS.                               
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR25.
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR25.                   
          INITIALIZE SUBSCRIPTS.                               
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR26.
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR26.                   
          INITIALIZE SUBSCRIPTS.                               
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR27.
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR27.                   
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR28. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR28.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR29. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR29.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR30. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR30.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR31. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR31.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR32. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR32.                      
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR33. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR33.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR34. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR34.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR35. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR35.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR36. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR36.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR37. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR37.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR38. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR38.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR39. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR39.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR40. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR40.                    
          INITIALIZE SUBSCRIPTS.                                
          READ ENCODEFILE.                                      
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR01. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR41.                    
          INITIALIZE SUBSCRIPTS.                                  
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR02. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR42.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR03. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR43.                    
          INITIALIZE SUBSCRIPTS.                                      
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR04. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR44.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR05. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR45.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR06. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR46.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR07. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR47.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR08. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR48.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR09. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR49.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR10.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR50.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR11. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR51.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR12. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR52.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR13. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR53.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR14. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR54. 
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR15. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR55.                     
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR16. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR56.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR17. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR57.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR18. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR58.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR19. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR59.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR20. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR60.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR21. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR61.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR22. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR62.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR23.   
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR63.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR24. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR64.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR25. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR65.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR26. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR66.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR27. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR67.                    
          INITIALIZE SUBSCRIPTS.                                
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR28. 
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR68.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR29.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR69.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR30.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR70.                       
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR31.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR71.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR32.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR72.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR33.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR73.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR34.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR74.                      
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR35.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR75.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR36.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR76.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR37.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR77.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR38.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR78.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR39.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR79.                     
          INITIALIZE SUBSCRIPTS.                                 
          PERFORM 0325-DECODE UNTIL NUM-CODE(A-SUB) = EN-CHR40.  
          MOVE ABC-CHAR(A-SUB) TO WS-DCHR80.                     
          INITIALIZE SUBSCRIPTS.                                 
          WRITE DECODE-RECORD FROM WS-DECODED-MSG1.              
                                                              
    0325-DECODE.                                
         ADD 1                 TO A-SUB. 
           
    0400-END-ROUTINE.                           
         CLOSE LETTERFILE ENCODEFILE DECODEFILE.    
         STOP RUN.   
